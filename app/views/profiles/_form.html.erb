<%= form_with(model: profile, local: true) do |form| %>
  <% if profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
      <% profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :firstname %>
    <%= form.text_field :firstname, id: :profile_firstname %>
  </div>

  <div class="field">
    <%= form.label :lastname %>
    <%= form.text_field :lastname, id: :profile_lastname %>
  </div>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address, id: :profile_address %>
  </div>

  <div class="field">
    <%= form.label :county %>
    <%= form.text_field :county, id: :profile_county %>
  </div>

  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email, id: :profile_email %>
  </div>

  <div class="field">
    <%= form.label :phone %>
    <%= form.number_field :phone, id: :profile_phone %>
  </div>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id, id: :profile_user_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
